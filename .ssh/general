Host *
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%h-%p-%r
    ControlPersist 72h
    ServerAliveInterval=15
    AddKeysToAgent yes
    UseKeychain yes
    ForwardAgent yes
    StrictHostKeyChecking=no
    UserKnownHostsFile /dev/null

    IdentityFile ~/.ssh/IdentityFile/Identity-nopasswd
    IdentityFile ~/.ssh/IdentityFile/kbsonlong.pem
    IdentityFile ~/.ssh/IdentityFile/zhifu.pem
    IdentityFile ~/.ssh/IdentityFile/git
